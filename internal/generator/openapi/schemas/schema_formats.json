{
  "$id": "schema_formats.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "description": "available formats for the property and filename",
  "properties": {
    "filename": {
      "enum": [
        "snake_case"
      ]
    },
    "property": {
      "enum": [
        "snake_case",
        "camelCase",
        "pascalCase",
        "kebab-case",
        "ENV"
      ]
    }
  }
}
